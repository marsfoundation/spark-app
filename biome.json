{
  "$schema": "https://biomejs.dev/schemas/1.8.1/schema.json",
  "extends": ["./biome.strict.json"],
  "linter": {
    "enabled": true,
    "rules": {
      "style": {
        "useNodejsImportProtocol": "off",
        "noUselessElse": "off",
        "useConst": "off"
      },
      "suspicious": {
        "noConsoleLog": "off",
        "noDebugger": "off"
      },
      "correctness": {
        "noUnusedImports": "off",
        "noUnusedVariables": {
          "level": "off"
        }
      }
    }
  }
}
